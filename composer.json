{
    "name": "rudi97277/export-db",
    "description": "Export data to XLSX or CSV formats from database queries or functions",
    "license": "MIT",
    "keywords": [
        "laravel",
        "php",
        "phpspreadsheet",
        "phpexcel",
        "excel",
        "csv",
        "database export"
    ],
    "require": {
        "maatwebsite/excel": "^3.1",
        "illuminate/database": "^10.48",
        "illuminate/http": "^10.48"
    },
    "autoload": {
        "psr-4": {
            "Rudi97277\\ExportDb\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Rudianto Sihombing",
            "email": "rudi97277@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Rudi97277\\ExportDb\\ExportServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
